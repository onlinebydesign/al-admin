{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "coreui-angular": {
      "root": "",
      "sourceRoot": "core",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "core/index.html",
            "main": "core/main.ts",
            "tsConfig": "core/tsconfig.app.json",
            "polyfills": "core/polyfills.ts",
            "assets": [
              "core/assets"
            ],
            "styles": [
              "core/scss/style.scss",
              "node_modules/font-awesome/css/font-awesome.css"
            ],
            "scripts": [
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/chart.js/dist/Chart.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "core/environments/environment.ts",
                  "with": "core/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "coreui-angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "coreui-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "coreui-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "core/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "core/polyfills.ts",
            "tsConfig": "core/tsconfig.spec.json",
            "scripts": [
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/chart.js/dist/Chart.min.js"
            ],
            "styles": [
              "core/scss/style.scss",
              "node_modules/font-awesome/css/font-awesome.css"
            ],
            "assets": [
              "core/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "core/tsconfig.app.json",
              "core/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "coreui-angular-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "coreui-angular:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "coreui-angular",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "al",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "al"
    }
  }
}